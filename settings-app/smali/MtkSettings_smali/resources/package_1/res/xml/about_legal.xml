<?xml version='1.0' encoding='utf-8' ?>
<PreferenceScreen android:title="@string/legal_information"
                  android:key="legal_screen" xmlns:android="http://schemas.android.com/apk/res/android">
  <Preference android:title="@string/copyright_title"
              android:key="copyright">
    <intent android:action="android.settings.COPYRIGHT" />
  </Preference>
  <Preference android:title="@string/license_title"
              android:key="license">
    <intent android:action="android.settings.LICENSE" />
  </Preference>
  <Preference android:title="@string/terms_title"
              android:key="terms">
    <intent android:action="android.settings.TERMS" />
  </Preference>
  <Preference android:title="@string/webview_license_title"
              android:key="webview_license">
    <intent android:action="android.settings.WEBVIEW_LICENSE" />
  </Preference>
  <Preference android:key="legal_item">
    <intent android:action="@string/legal_item" />
  </Preference>
  <Preference android:title="privacy policy"
              android:key="privacy_apr_item">
    <intent android:action="com.fihtdc.AprUploadService.action.startPrivacyPolicy" />
  </Preference>
  <Preference android:key="legal_apr_item">
    <intent android:action="@string/legal_apr_item" />
  </Preference>
  <Preference android:title="@string/zzz_hmd_open_source_info_title"
              android:key="hmd_open_source_info">
    <intent android:action="android.settings.HMD_OPEN_SOURCE" />
  </Preference>
  <Preference android:title="@string/wallpaper_attributions"
              android:key="wallpaper_attributions"
              android:summary="@string/wallpaper_attributions_values" />
</PreferenceScreen>